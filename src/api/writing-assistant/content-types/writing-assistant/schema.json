{
  "kind": "collectionType",
  "collectionName": "writing_assistants",
  "info": {
    "singularName": "writing-assistant",
    "pluralName": "writing-assistants",
    "displayName": "Writing Assistant",
    "description": "Landing pages for writing assistants"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "profession": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "page_title",
      "required": true
    },
    "hero_title": {
      "type": "string",
      "required": true
    },
    "hero_subtitle": {
      "type": "richtext"
    },
    "blocks": {
      "type": "dynamiczone",
      "components": [
        "shared.rich-text",
        "shared.quote",
        "shared.media",
        "shared.slider"
      ]
    },
    "seo": {
      "type": "component",
      "component": "shared.seo",
      "repeatable": false
    },
    "page_title": {
      "type": "string"
    }
  }
}
